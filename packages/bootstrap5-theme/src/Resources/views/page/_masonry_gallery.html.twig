{% if page.images is defined and page.images is not empty %}

    {% set images = page.images.slice(galleryFilterFrom|default('0'), galleryFilterTo|default('1000')) %}

    {% set id = 'gallery' %}
    {% set container = true %}
        {% block body %}
            <div class="un-masonry masonry mimg">
                {% for image in images %}
                    {% include '@Pushword/page/_masonry_gallery_part.html.twig' with {'image': image} only %}
                {% endfor %}
            </div>
        {% endblock %}
{% endif %}
