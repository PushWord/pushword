{
    "name": "pushword/pushword",
    "replace": {
        "piedweb/theme-component-bundle": "self.version",
        "pushword/admin": "self.version",
        "pushword/conversation": "self.version",
        "pushword/core": "self.version",
        "pushword/flat-file": "self.version",
        "pushword/page-scanner": "self.version",
        "pushword/page-update-notifier": "self.version",
        "pushword/static-generator": "self.version",
        "pushword/template-editor": "self.version"
    },
    "description": "Monorepo for pushword CMS and supported extensions.",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "caseyamcl/toc": "^3.0",
        "cocur/slugify": "^3.0|^4.0",
        "doctrine/annotations": "^1.11",
        "doctrine/doctrine-bundle": "^2.2.2",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.8.1",
        "fortawesome/font-awesome": "^5.15",
        "intervention/image": "^2.5",
        "knplabs/knp-markdown-bundle": "^1.8",
        "league/color-extractor": "^0.3.2",
        "league/commonmark": "^1.5",
        "miljar/php-exif": "^0.6.5",
        "nette/utils": "^3.2",
        "piedweb/facebook-scraper": "^0.0.72",
        "piedweb/linksimprover": "^0.0.8",
        "piedweb/render-html-attributes": "^2.0.1",
        "piedweb/url-harvester": "^0.0.27",
        "sensio/framework-extra-bundle": "^5.6",
        "sonata-project/admin-bundle": "dev-master#f34ae30d169132b17f3151fa294a80c57247a8a9",
        "sonata-project/doctrine-orm-admin-bundle": "4.x-dev",
        "spatie/image-optimizer": "^1.3",
        "spatie/yaml-front-matter": "^2.0",
        "symfony/asset": "^5.2",
        "symfony/cache": "^5.2",
        "symfony/console": "^5.2",
        "symfony/expression-language": "^5.2",
        "symfony/finder": "^5.2",
        "symfony/form": "^5.2",
        "symfony/framework-bundle": "^5.2",
        "symfony/lock": "^5.2",
        "symfony/mailer": "^5.2",
        "symfony/process": "^5.2",
        "symfony/security-bundle": "^5.2",
        "symfony/security-csrf": "^5.2",
        "symfony/security-guard": "^5.2",
        "symfony/serializer": "^5.2",
        "symfony/translation": "^5.2",
        "symfony/twig-bundle": "^5.2",
        "symfony/validator": "^5.2",
        "symfony/web-link": "^5.2",
        "symfony/webpack-encore-bundle": "^1.8",
        "symfony/yaml": "^5.2",
        "twig/intl-extra": "^3.2",
        "vich/uploader-bundle": "^1.16",
        "villfa/invert-color": "^0.5",
        "wyrihaximus/html-compress": "^4.1"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "friendsofphp/php-cs-fixer": "^2.17",
        "friendsoftwig/twigcs": "^4.0",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/dotenv": "^5.2",
        "symfony/maker-bundle": "^1.26",
        "symfony/phpunit-bridge": "^5.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0",
        "symplify/monorepo-builder": "^9.0",
        "symplify/vendor-patches": "^9.0",
        "vimeo/psalm": "^4.3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "PiedWeb\\BootstrapThemeComponentBundle\\": "packages/bootstrap5-theme/src/",
            "Pushword\\Admin\\": "packages/admin/src",
            "Pushword\\Conversation\\": "packages/conversation/src",
            "Pushword\\Core\\": "packages/core/src",
            "Pushword\\Flat\\": "packages/flat/src",
            "Pushword\\PageScanner\\": "packages/page-scanner/src",
            "Pushword\\PageUpdateNotifier\\": "packages/page-update-notifier/src",
            "Pushword\\StaticGenerator\\": "packages/static-generator/src",
            "Pushword\\Svg\\": "packages/svg/src",
            "Pushword\\Facebook\\": "packages/facebook/src",
            "Pushword\\Version\\": "packages/version/src",
            "Pushword\\TemplateEditor\\": "packages/template-editor/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "packages/skeleton/src",
            "Pushword\\Admin\\Tests\\": "packages/admin/tests",
            "Pushword\\Conversation\\Tests\\": "packages/conversation/tests",
            "Pushword\\Core\\Tests\\": "packages/core/tests",
            "Pushword\\Flat\\Tests\\": "packages/flat/tests",
            "Pushword\\PageScanner\\Tests\\": "packages/page-scanner/tests",
            "Pushword\\PageUpdateNotifier\\Tests\\": "packages/page-update-notifier/tests",
            "Pushword\\StaticGenerator\\Tests\\": "packages/static-generator/tests",
            "Pushword\\Svg\\Tests\\": "packages/svg/tests",
            "Pushword\\Facebook\\Tests\\": "packages/facebook/tests",
            "Pushword\\Version\\Tests\\": "packages/Version/tests",
            "Pushword\\TemplateEditor\\Tests\\": "packages/template-editor/tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm --no-suggestions",
        "test": "vendor/bin/phpunit",
        "tests": "composer update --prefer-lowest && composer test && composer update && composer test",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix",
        "assets-core": "cd packages/core && yarn && yarn build && cd ../",
        "assets-admin": "cd packages/admin && yarn && cd ../../ && php ./packages/admin/src/Resources/assets/easymde-hack && cd packages/admin && yarn build",
        "assets": "composer assets-core && composer assets-admin",
        "docs": "cd packages/docs && ./build",
        "publish-js-helper": "cd packages/js-helper && npm version patch && cd ../../ && git add packages/js-helper/. && git commit -m 'update pushword/js-helper version' && git push && cd packages/js-helper && npm publish",
        "reset-skeleton": "cd packages/skeleton && rm var/app.db && php bin/console doctrine:schema:create -q && php bin/console doctrine:fixtures:load -q && php bin/console pushword:image:cache -q && php bin/console pushword:user:create admin@example.tld p@ssword ROLE_SUPER_ADMIN"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
